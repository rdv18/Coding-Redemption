class Solution
{
    //Function to reverse a linked list.
    Node reverseList(Node head)
    {
        // code here
        Node temp;
        Node prev=null;;
        while(head!=null)
        {
            temp=head.next;
            head.next=prev;
            prev=head;
            head=temp;
            
        }
        
        return prev;
    }
}
